{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "plugins": [
    "vitest"
  ],
  "rules": {
  },
  "overrides": [
    {
      "files": ["src/setupTests.js"],
      "env": {
        "node": true
      }
    }
  ]
}
